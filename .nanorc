# Slackjeff nano Preset ;)
#===========================

# Habilita ajuste suave de linhas compridas
set softwrap

# Nï¿½mero de tabs
set tabsize 4

# Desabilita a insercaoo de ultima linha
set nonewlines

# Desabilita modo de ajuda.
set nohelp

# Mostra informacoes (col/line/cursor)
set constantshow

# Auto Identacao
set autoindent

# Tab para backspace
set tabstospaces

# Multiplos buffers por padrao
set multibuffer

# set element fgcolor,bgcolor

set titlecolor brightgreen,black

set statuscolor brightgreen,black

set keycolor cyan

set functioncolor brightgreen